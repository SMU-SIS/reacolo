{
  "name": "reacolo",
  "version": "0.4.2",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint 'app/**/*.@(js|jsx)'",
    "lint:msg": "validate-commit-msg",
    "test": "jest",
    "build": "NODE_ENV=production webpack --config webpack.config.js -p",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "postrewrite": "npm run git-hook",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "commitmsg": "npm run lint:msg",
    "git-hook": "npm run lint"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "sockjs-client": "^1.1.1",
    "eventemitter3": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "commitizen": "^2.9.5",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.6.0",
    "husky": "^0.12.0",
    "jest": "^18.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "validate-commit-msg": "^2.8.2",
    "webpack": "^2.2.0"
  },
  "keywords": [
    "ecology",
    "cross-devices"
  ],
  "author": "Quentin Roy <quentinroy@smu.edu.sg>",
  "license": "UNLICENSED",
  "private": true,
  "repository": "bitbucket:quentinroy/reacolo-client",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "build",
        "ci"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "es6-error": "^4.0.1",
    "eslint-plugin-react": "^6.9.0",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2"
  }
}
