{
  "name": "@reacolo/match",
  "main": "lib/reacolo-match.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rollup -c --environment NODE_ENV:production",
    "prepublish": "in-publish && yarn run build || not-in-publish"
  },
  "files": ["lib"],
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "in-publish": "^2.0.0",
    "jest": "^22.0.3",
    "pegjs": "^0.10.0",
    "rollup": "^0.53.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-pegjs": "^2.1.3"
  },
  "keywords": ["ecology", "adaptation", "match", "context", "reacolo"],
  "author": "Quentin Roy <quentin@quentinroy.fr>",
  "license": "MIT",
  "publishConfig": {
    "registry": "http://localhost:4873",
    "access": "restricted"
  },
  "jest": {
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.pegjs$": "<rootDir>/jest-tools/pegjs-transformer.js"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "version": "5.0.0-alpha.0"
}
